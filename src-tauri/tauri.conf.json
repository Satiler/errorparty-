{
    "productName":  "ErrorParty",
    "version":  "1.0.0",
    "identifier":  "ru.errorparty.app",
    "build":  {
                  "beforeDevCommand":  "cd frontend \u0026\u0026 npm run dev",
                  "beforeBuildCommand":  "cd frontend \u0026\u0026 npm run build",
                  "devUrl":  "http://localhost:5173",
                  "frontendDist":  "../frontend/dist"
              },
    "bundle":  {
                   "active":  true,
                   "targets":  "all",
                   "icon":  [
                                "icons/icon.png",
                                "icons/icon.ico"
                            ],
                   "resources":  [

                                 ],
                   "copyright":  "Copyright © 2025 ErrorParty Team",
                   "category":  "Music",
                   "shortDescription":  "Музыкальный плеер ErrorParty",
                   "longDescription":  "ErrorParty.ru Desktop - полнофункциональный музыкальный плеер"
               },
    "plugins":  {

                },
    "app":  {
                "windows":  [
                                {
                                    "title":  "ErrorParty",
                                    "width":  1280,
                                    "height":  800,
                                    "minWidth":  900,
                                    "minHeight":  600,
                                    "resizable":  true,
                                    "fullscreen":  false,
                                    "center":  true
                                }
                            ],
                "security":  {
                                 "csp":  null,
                                 "dangerousDisableAssetCspModification": true,
                                 "devCsp": null,
                                 "freezePrototype": false,
                                 "assetProtocol": {
                                     "enable": true,
                                     "scope": ["**"]
                                 }
                             },
            "withGlobalTauri": true
            }
}
